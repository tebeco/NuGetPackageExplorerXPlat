{
  "name": "nuget-package-explorer-front",
  "version": "0.0.1",
  "description": "Nuget Package Explorer, based on Electron and .NetCore",
  "keywords": [
    "nuget",
    "nuget package explorer",
    "npe",
    "xplat"
  ],
  "author": "TeBeCo",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "test",
    "build": "electron-builder --dir --win --x64"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.19.1",
    "rimraf": "^2.6.1"
  },
  "build": {
    "appId": "cross-platform-desktop",
    "directories": {
      "output": "../dist"
    },
    "extraResources": {
      "from": "../api/dist/",
      "to": "../api/",
      "filter": [
        "**/*"
      ]
    },
    "win": {
      "target": [
        "NSIS"
      ]
    }
  }
}