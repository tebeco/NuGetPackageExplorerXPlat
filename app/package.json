{
  "name": "nuget-package-explorer-front",
  "version": "0.0.1",
  "description": "Nuget Package Explorer, based on Electron and .NetCore",
  "main": "main.js",
  "scripts": {
    "test": "test",
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "nuget",
    "nuget package explorer",
    "npe",
    "xplat"
  ],
  "author": "TeBeCo",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.19.1"
  },
  "build": {
    "appId": "cross-platform-desktop",
    "directories": {
      "buildResources": "../../assets",
      "output": "../../dist"
    },
    "extraResources": {
      "from": "../api/src/NugetPackageExplorerXPlat/bin/dist/",
      "to": "api/bin/dist/",
      "filter": [
        "**/*"
      ]
    },
    "mac": {
      "category": "cross-platform-desktop"
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  }
}
